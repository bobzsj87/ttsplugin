{
  "manifest_version": 2,
  "name": "TTS Engine",
  "version": "0.1",
  "minimum_chrome_version": "23",
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "permissions": ["tts"],
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "externally_connectable": {
    "matches": ["*://dev.hergerman.com:*/*", "*://bobzsj87.github.io/her-german/*"]
  }
}
